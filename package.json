{
  "name": "connected-dataloader",
  "version": "1.0.0",
  "description": "Create data loaders connected to a common backing storage.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist/**"],
  "keywords": ["graphql", "dataloader"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sophiabits/connected-dataloader"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && tsc",
    "check-format": "prettier --check \"(src|test)/**/*.ts\"",
    "format": "prettier --write \"(src|test)/**/*.ts\"",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "test": "vitest run"
  },
  "dependencies": {
    "dataloader": "^2.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
